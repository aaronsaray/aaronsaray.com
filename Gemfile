source 'https://rubygems.org'

## Forcing this to an older version to that it works with github-pages at 183
gem "jekyll", "~> 3.7.3"

## Forcing github-pages to 183 which is the most recent release at this particular point
gem 'github-pages', '>=183', group: :jekyll_plugins

group :jekyll_plugins do
   gem "jekyll-paginate"
   gem "jekyll-sitemap"
   gem "jekyll-tagging"
end
